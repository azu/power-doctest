{
  "name": "comment-to-assert",
  "version": "1.5.1",
  "description": "convert line comment to assert.",
  "keywords": [
    "ast",
    "doctest",
    "testing"
  ],
  "homepage": "https://github.com/azu/comment-to-assert",
  "bugs": {
    "url": "https://github.com/azu/comment-to-assert/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/comment-to-assert.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "lib",
    "src",
    "bin"
  ],
  "main": "lib/comment-to-assert.js",
  "types": "lib/comment-to-assert.d.ts",
  "bin": {
    "comment-to-assert": "./bin/cmd.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "example": "npm i && npm run build && cd example && npm test",
    "postcommit": "git reset",
    "precommit": "lint-staged",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.{js,ts}\"",
    "watch": "tsc -p . --watch"
  },
  "dependencies": {
    "acorn": "^4.0.3",
    "concat-stream": "^1.5.0",
    "escodegen": "^1.6.1",
    "espree": "^3.1.5",
    "estraverse": "^4.1.0",
    "esutils": "^2.0.2",
    "tagged-template-to-ast": "^2.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "ast-equal": "^1.0.1",
    "cross-env": "^5.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "mocha": "^5.2.0",
    "power-assert": "^1.4.1",
    "prettier": "^1.6.1",
    "ts-node": "^7.0.1",
    "ts-node-test-register": "^4.0.0",
    "typescript": "^3.2.4"
  },
  "email": "azuciao@gmail.com",
  "lint-staged": {
    "*.{js,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
