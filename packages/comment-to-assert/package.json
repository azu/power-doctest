  },
  "version": "1.3.0",
  "description": "convert line comment to assert.",
  "main": "lib/comment-to-assert.js",
  "bin": {
    "comment-to-assert": "./bin/cmd.js"
  },
  "files": [
    "lib",
    "src",
    "bin"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha && npm run example",
    "example": "npm i && npm run build && cd example && npm test"
  },
  "keywords": [
    "ast",
    "testing",
    "doctest"
  ],
  "dependencies": {
    "acorn": "^3.2.0",
    "concat-stream": "^1.5.0",
    "escodegen": "^1.6.1",
    "espree": "^3.1.5",
    "estraverse": "^4.1.0",
    "esutils": "^2.0.2",
    "tagged-template-to-ast": "^2.0.0"
  },
  "devDependencies": {
    "ast-equal": "^1.0.1",
    "babel-cli": "^6.10.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "mocha": "^2.2.5",
    "power-assert": "^1.4.1"
  }
