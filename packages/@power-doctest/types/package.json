{
	"name": "@power-doctest/types",
	"version": "5.3.5",
	"description": "Shared types of @power-doctest",
	"keywords": [
		"types",
		"typescript"
	],
	"homepage": "https://github.com/azu/power-doctest/tree/master/packages/@power-doctest/types/",
	"bugs": {
		"url": "https://github.com/azu/power-doctest/issues"
	},
	"repository": {
