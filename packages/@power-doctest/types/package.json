    "cross-env": "^7.0.3",
    "mocha": "^8.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.5.4"
