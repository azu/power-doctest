{
  "name": "@power-doctest/asciidoctor",
  "version": "1.0.0",
  "description": "A power-doctest Runner using for Asciidoctor.",
  "keywords": [
    "asciidoc",
    "asciidoctor",
    "doctest"
  ],
  "homepage": "https://github.com/azu/power-doctest/tree/master/packages/@power-doctest/asciidoctor/",
  "bugs": {
    "url": "https://github.com/azu/power-doctest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/power-doctest.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "asciidoctor": "^2.0.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.3",
    "cross-env": "^5.2.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
