{
    "name": "@power-doctest/javascript",
    "version": "5.3.5",
    "description": "A JavaScript parser for power-doctest.",
    "keywords": [
        "parser",
        "power-doctest"
    ],
    "homepage": "https://github.com/azu/power-doctest/tree/master/packages/@power-doctest/javascript/",
    "bugs": {
        "url": "https://github.com/azu/power-doctest/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/azu/power-doctest.git"
    },
    "license": "MIT",
    "author": "azu",
    "files": [
        "bin/",
        "lib/",
        "src/"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "directories": {
        "lib": "lib",
        "test": "test"
    },
    "scripts": {
        "build": "tsc -p .",
        "updateSnapshot": "UPDATE_SNAPSHOT=1 npm test",
        "clean": "rimraf lib/",
        "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
        "prepublish": "npm run --if-present build",
        "test": "mocha \"test/**/*.ts\"",
        "watch": "tsc -p . --watch"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": false,
        "tabWidth": 4
    },
    "dependencies": {
        "@power-doctest/types": "workspace:^",
        "structured-source": "^4.0.0"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node": "^24.0.10",
        "mocha": "^11.7.1",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "ts-node": "^10.4.0",
        "ts-node-test-register": "^10.0.0",
        "typescript": "^5.8.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
