{
  "name": "@power-doctest/javascript",
  "version": "5.2.0",
  "description": "A JavaScript parser for power-doctest.",
  "keywords": [
    "parser",
    "power-doctest"
  ],
  "homepage": "https://github.com/azu/power-doctest/tree/master/packages/@power-doctest/javascript/",
  "bugs": {
    "url": "https://github.com/azu/power-doctest/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/power-doctest.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "updateSnapshot": "cross-env UPDATE_SNAPSHOT=1 npm test",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@power-doctest/types": "^5.2.0",
    "@types/structured-source": "^3.0.0",
    "structured-source": "^3.0.2"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "cross-env": "^7.0.3",
    "mocha": "^8.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-test-register": "^9.0.1",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
